{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/pages/**/*.{ts,tsx}",
    "src/client/features/index.ts",
    "src/client/routes/index.ts",
    "src/apis/index.ts",
    "src/agents/index.ts",
    "scripts/**/*.{ts,js}",
    "config/**/*.{ts,mjs,js}",
    "eslint.config.mjs"
  ],
  "project": [
    "src/**/*.{ts,tsx}",
    "scripts/**/*.{ts,js}",
    "config/**/*.{ts,mjs,js}"
  ],
  "ignore": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.d.ts",
    "src/pages/api/**",
    "node_modules/**",
    "src/agents/lib/**",
    "src/server/s3/example.ts"
  ],
  "ignoreDependencies": [
    "@types/*",
    "tailwindcss",
    "@tailwindcss/*",
    "postcss",
    "autoprefixer",
    "sharp",
    "tsx",
    "@eslint/eslintrc",
    "eslint-config-next",
    "eslint-plugin-boundaries",
    "@playwright/mcp",
    "@anthropic-ai/claude-code",
    "aws-sdk",
    "date-fns",
    "undici"
  ],
  "ignoreBinaries": [
    "madge"
  ],
  "ignoreExportsUsedInFile": true,
  "next": {
    "entry": [
      "src/pages/**/*.{ts,tsx}",
      "next.config.ts",
      "config/next/*.ts"
    ]
  }
}
